<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="root" prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.fx.controller.AttendenceTableController">
   <children>
      <TableView fx:id="tblAttendance" layoutX="-1.0" layoutY="48.0" prefHeight="551.0" prefWidth="876.0">
